{
  "name": "lib-random",
  "version": "1.0.2",
  "description": "A seeded pseudorandom number generator from the PCG family with support for various distributions. A suite of utility functions are also included.",
  "keywords": [
      "pcg",
      "random",
      "seed",
      "gaussian",
      "normal",
      "rng"
    ],
  "main": "./lib/lib-random.min.js",
  "scripts": {
    "clear": "rm -rf node_modules/",
    "test": "babel-node spec/run.js",
    "build": "babel src -o lib/lib-random.js",
    "build-min": "BABEL_ENV=production babel src -o lib/lib-random.min.js"
  },
  "author": "Josh D. Dunn",
  "repository": {
      "type": "git",
      "url": "https://github.com/joshddunn/lib-random.git"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-babili": "^0.1.4",
    "babel-preset-es2015": "^6.24.1",
    "jasmine": "^2.7.0"
  },
  "dependencies": {
    "long": "^3.2.0"
  },
  "homepage": "https://github.com/joshddunn/lib-random",
  "bugs": {
    "url": "https://github.com/joshddunn/lib-random/issues"
  }
}
